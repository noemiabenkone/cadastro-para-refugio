generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
}

model Cadastro {
  id               Int      @id @default(autoincrement())
  nome             String
  sobreNome        String
  dataDeNascimento String
  paisDeOrigem     String
  cidade           String
  nomeDaMae        String
  email            String
  createdAt        DateTime @default(now())
}
